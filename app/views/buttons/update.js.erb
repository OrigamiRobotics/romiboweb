<% if params[:status] == 'done' %>
  $('#doneButton').removeAttr('href');
  $('#doneButton').removeClass('btn-primary');
  $('#doneButton').addClass('btn-default');
  $('#doneButton').click(function(){
    return false;
  });
<% else %>
//  $('#paletteButtonsInCenterColumn').replaceWith('<!%= j render "palettes/center_content/existing_buttons" %>');
  $('<%= "#button-#{@button.id}" %>').replaceWith('<%= j render "buttons/gridster/widget", button: @button %>');
  <%= render 'buttons/update_and_show' %>
<% end %>

