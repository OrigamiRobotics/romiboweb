<%= render 'buttons/update_and_show' %>
$('#doneButton').hide();
<%= render 'buttons/move_focus_to_title_field' %>
<%  if @palette.current_button.present? && Button.exists?(@palette.current_button.id) %>
  $("#<%= "button-#{@palette.current_button.id}"%>").addClass('btn-selected') ;
<% else %>
  $('#buttonDetails').hide();
<% end %>

