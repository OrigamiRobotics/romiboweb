<% provide(:title, 'Home') %>

<% content_for :content  do %>
	<div class="container">
	  <%= render 'romiboweb_pages/home/welcome' %>
		<div class="row-fluid">
			<div class="col-md-5">
				<div class="panel panel-info">
					<div class="panel-heading">
					  <h3 class="panel-title center">Already a member? Sign In</h3>
					</div>
					<div class="panel-body">
					  <%= render 'devise/sessions/form' %>
					</div>
				</div>
			</div>
			<div class="col-md-5 col-md-offset-2 center">
			  <div class="panel panel-info">
			    <div class="panel-heading">
			      <h3 class="panel-title center">New to RomiboWeb? Sign Up</h3>
			    </div>
				  <div class="panel-body">
				    <%= render 'devise/registrations/form' %>
				  </div>
			  </div>
			</div>
		</div>
	</div>

<% end %>