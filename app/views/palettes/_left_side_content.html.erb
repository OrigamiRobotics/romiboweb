<div class="editorContent" id="leftSideContentBox">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title">
				<%= 'My Palettes' %>
			  <%= link_to '<span class="glyphicon glyphicon-plus"></span>'.html_safe,
			              new_palette_path, remote: true, class: 'pull-right' %>
			</h3>
		</div>
		<div id="paletteList" class="panel-body">
		  <%= render 'palettes/list' %>
		</div>

	</div>

</div>