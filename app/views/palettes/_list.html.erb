<% if @palettes.any? %>
  <div class="btn-group-vertical" id="paletteGroup">
    <% @palettes.each do |palette| %>
      <%= link_to palette.title, palette, remote: true, format: :js,
                  class: 'btn btn-default' , id: "#{palette.title}-#{palette.id}" %>
    <% end %>
  </div>
<% end %>